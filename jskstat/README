The JavaScript page depends on jquery, jstree and flot

https://www.jstree.com/
https://www.flotcharts.org/

jstree:

unzip ../../vakata-jstree-3.3.17-0-g6256df0.zip
mv vakata-jstree-6256df0/dist/jstree.min.js .
mv vakata-jstree-6256df0/dist/themes .
rm -fr vakata-jstree-6256df0

flot:

unzip ../../flot-0.8.2.zip
rm -fr flot/examples
rm flot/*.md flot/Makefile
mkdir t
mv flot/*.min.js t
rm flot/*.js
mv t/*.js flot
rmdir t
rm flot/jquery.min.jsx
